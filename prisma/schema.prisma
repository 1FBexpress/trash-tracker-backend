generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id        String   @id @default(cuid())
  title     String
  status    String   @default("pending")
  pickerId  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
