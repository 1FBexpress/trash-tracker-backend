// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id        String   @id @default(uuid())
  title     String
  status    String   @default("OPEN")
  createdAt DateTime @default(now())
}
